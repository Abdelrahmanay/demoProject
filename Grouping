import numpy as np
import pandas as pd
import matplotlib.pyplot as plt

df = pd.read_excel(r'C:\Users\Ajith\Desktop\Techies\survey.xlsx')

df_abi= df[df['Was machen Sie zurzeit?'] == 'Abitur']
df_bachelor= df[df['Was machen Sie zurzeit?'] == 'Bachelor Studium']
df_master= df[df['Was machen Sie zurzeit?'] == 'Master Studium']
#Participants=P
P1 = "Number of Highschool students: " + str(df_abi.shape[0])
P2 = "Number of Bachelor students: " + str(df_bachelor.shape[0])
P3 = "Number of Master students: " + str(df_master.shape[0])

print(P1 + " \n" + P2 + " \n" + P3 )
